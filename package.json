{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.11",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "node ./scripts/write-robots-txt.js --env dev && react-scripts build",
    "build:prod": "node ./scripts/write-robots-txt.js --env prod && react-scripts build",
    "test": "react-scripts test",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:build-prod": "docker-compose -f docker-compose.prod.yml build",
    "docker:start-prod": "docker run -p 80:80 --name study-rent-prod study-rent-prod",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*{.ts,.tsx,.js,.jsx}\"",
    "lint:fix": "eslint --fix",
    "lint:style": "stylelint --fix \"src/**/*.scss\"  --custom-syntax postcss-scss",
    "prettier": "prettier --write \"src/**/*{js,jsx,ts,tsx,css,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4"
  }
}
